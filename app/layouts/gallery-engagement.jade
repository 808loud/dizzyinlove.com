-
  var gallerySlides = [
    'cuddyphoto-023.jpg', 
    'cuddyphoto-062.jpg',
    'cuddyphoto-042.jpg',
    'cuddyphoto-068.jpg',
    'cuddyphoto-078.jpg',
    'cuddyphoto-120.jpg',
    'cuddyphoto-124.jpg',
    'cuddyphoto-080.jpg'
  ];
- var imgPath = 'images/'
- var activeSlide = '0'

  
//- .container-fluid
//-   .row
#gallery_engagement.carousel.slide(data-ride='carousel')
  // Indicators
  ol.carousel-indicators
    - for (var i = 0; i < gallerySlides.length; i++)
      - defaultSlide = (i == '0' ? 'active' : '')
      li(data-target='#gallery_engagement' data-slide-to='#{i}' class='#{defaultSlide}')
  // Slides
  .carousel-inner(role='listbox')
    - for (var i = 0; i < gallerySlides.length; i++)
      - defaultSlide = (i == '0' ? 'active' : '')
      .item(class='#{defaultSlide}')
        img(src='#{imgPath}#{gallerySlides[i]}' alt='alt')
  // Controls
  a.left.carousel-control(href='#gallery_engagement' role='button' data-slide='prev')
    span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
    span.sr-only Previous
  a.right.carousel-control(href='#gallery_engagement' role='button' data-slide='next')
    span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
    span.sr-only Next
